{
  "name": "apollo-opentracing",
  "version": "0.0.0-development",
  "description": "Trace your GraphQL server with Opentracing",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc",
    "prepare": "npm run clean && npm run compile",
    "test": "jest",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once --pro",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": "DanielMSchmidt/apollo-opentracing",
  "author": "Daniel Schmidt <danielmschmidt92@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "apollo-server-env": "*",
    "graphql": ">=0.10.x",
    "opentracing": "*",
    "apollo-server": ">=2.0.0"
  },
  "dependencies": {
    "apollo-server-types": "^0.6.0",
    "apollo-server-plugin-base": "^0.10.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.2",
    "@types/supertest": "2.0.10",
    "all-contributors-cli": "6.19.0",
    "apollo-server": "2.18.2",
    "apollo-server-env": "2.4.5",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "graphql-tools": "6.2.5",
    "jest": "26.6.1",
    "opentracing": "0.14.4",
    "semantic-release": "17.2.1",
    "supertest": "5.0.0",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3"
  }
}
